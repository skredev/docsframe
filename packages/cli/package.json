{
  "name": "@chosera/docsframe-cli",
  "version": "0.0.1",
  "description": "As simple as a NextJS documentation framework can get",
  "keywords": [
    "chosera",
    "docsframe",
    "documentation",
    "shadcn-ui"
  ],
  "homepage": "https://docsframe.work",
  "bugs": {
    "url": "https://github.com/skredev/docsframe/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skredev/docsframe.git",
    "directory": "packages/cli"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "release": "pnpm build && pnpm publish --access public"
  },
  "main": "./dist/index.ts",
  "author": "skredev",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "tsc": "^2.0.4",
    "tsup": "^8.3.0"
  }
}
